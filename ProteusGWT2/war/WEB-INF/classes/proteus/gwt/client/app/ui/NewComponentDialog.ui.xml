<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		
		.panel{
			margin-left: 4px;
			margin-right: 4px;
			margin-top: 4px;
		}
	</ui:style>

	<g:DialogBox ui:field="box" text="New Model" animationEnabled="true">
		<g:VerticalPanel >
			
			<g:HorizontalPanel addStyleNames="{style.panel}">
				<g:Label width="100px">Name:</g:Label>
				<g:TextBox ui:field="tbName" width="150px"></g:TextBox>				
			</g:HorizontalPanel >
			
			<g:HorizontalPanel addStyleNames="{style.panel}">
				<g:Label width="100px">Description:</g:Label>
				<g:TextBox ui:field="tbDescription" width="150px"></g:TextBox>
			</g:HorizontalPanel>	
			<g:HorizontalPanel addStyleNames="{style.panel}">
				<g:Label width="100px">Final:</g:Label>
				<g:SimpleCheckBox  ui:field="final_"/>
			</g:HorizontalPanel>	
			
			<g:HorizontalPanel addStyleNames="{style.panel}">
				<g:Label width="100px">Encapsulated:</g:Label>
				<g:SimpleCheckBox  ui:field="encapsulated" />
			</g:HorizontalPanel>	
			<g:HorizontalPanel addStyleNames="{style.panel}">
				<g:Label width="100px">Partial:</g:Label>
				<g:SimpleCheckBox ui:field="partial" />
			</g:HorizontalPanel>	
			
			
			
			<g:HorizontalPanel styleName="{style.panel}">
				<g:Button ui:field="btOk">OK</g:Button>
				<g:Button ui:field="btCancel">Cancel</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>
</ui:UiBinder> 