<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.pushbutton {
			border: 0;
			border-color: #fff;
			background: #fff;
			 padding: 1px;
		}
		.tabText {
			width:auto;
		}
	</ui:style>
	<g:HorizontalPanel>
		<g:Label ui:field="tabText" addStyleNames="{style.tabText}" wordWrap="false"/>
		<g:Image ui:field="image" url="resources/images/close.png"/>

	</g:HorizontalPanel>
</ui:UiBinder> 