<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.html{
			padding : 2px;
		}
		.panel{
			width: auto;
			border: 1px solid  #bbb;			
			margin: 2px;
			border-radius: 5px;
		}
	</ui:style>

	<g:HTMLPanel ui:field="scrollPanel" styleName="{style.panel}">
		<g:Frame  width="100%" height="100%" ui:field="fram"/>
	</g:HTMLPanel>

</ui:UiBinder> 